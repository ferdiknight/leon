cleo.search.network.typeahead.config.name=user04
cleo.search.network.typeahead.config.partition.start=3000000
cleo.search.network.typeahead.config.partition.count=1000000
cleo.search.network.typeahead.config.homeDir=network-typeahead/user/i004
